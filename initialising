class Main { 
    public static void main(String[] args) {
    {
        String first="Hello"; 
        System.out.println(first);
        String second="Bye"; 
        System.out.println(second); 
        first.concat(second);
        second.concat(first);
        System.out.println(first);
        System.out.println(second);
    } 
    
}
    
}
